{
  "private": true,
  "name": "wheat-leaf-disease-detection",
  "description": "Wheat Leaf Disease Detection with Model",
  "version": "1.0.0",
  "author": "Taiba riaz",
  "main": "app/background.js",
  "scripts": {
    "dev": "nextron",
    "build":"next build",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.example.wheat_leaf",
    "productName": "Wheat Leaf Disease Detection",
    "copyright": "Copyright Â© 2025",
    "directories": {
      "output": "dist",
      "buildResources": "resources"
    },
    "files": [
      {
        "from": ".",
        "filter": [
          "package.json",
          "app",
          "resources",
          "src/types"
        ]
      }
    ],
    "extraResources": [
      {
        "from": "resources",
        "to": "resources",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "build/vc_redist.x64.exe",
        "to": "vc_redist.x64.exe"
      }
    ],
    "nsis": {
      "include": "installer.nsh"
    },
    "publish": null
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-serve": "^1.3.0",
    "electron-store": "^8.2.0",
    "lucide-react": "^0.488.0",
    "radix-ui": "^1.2.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.52",
    "autoprefixer": "^10.4.19",
    "electron": "^34.0.0",
    "electron-builder": "^24.13.3",
    "next": "^14.2.4",
    "nextron": "^9.5.0",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.7.3"
  }
}
